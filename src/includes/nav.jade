- var items = []
- items.push({ href: '#intro', text: 'About me' })
- items.push({ href: '#projects', text: 'Projects' })
- items.push({ href: '#contact', text: 'Contact' })
nav.white(role='navigation')
  .nav-wrapper.container
    a#logo-container.brand-logo(href='#!') portfolio

    ul.right.hide-on-med-and-down
      for item in items
        li
          a(href=item.href) #{item.text}

    #nav-mobile.side-nav
      //img.responsive-img(src="assets/images/background9.jpg")
      ul.table-of-contents
        for item in items
          li
            a(href=item.href) #{item.text}

    a.button-collapse(href='#!', data-activates='nav-mobile')
      i.mdi-navigation-menu
